{
  "name": "message",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "deliveryReceipts",
        "readReceipts"
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "text": {
      "type": "string"
    },
    "type": {
      "type": "string",
      "default": "user"
    },
    "mediaUrl": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "peer": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "message_has_owner",
      "keyThrough": "targetId"
    },
    "room": {
      "type": "hasMany",
      "model": "room",
      "foreignKey": "targetId",
      "through": "room_has_message",
      "keyThrough": "sourceId"
    },
    "deliveryReceipts": {
      "type": "hasMany",
      "model": "delivery",
      "foreignKey": "sourceId",
      "through": "message_has_deliveryreceipt",
      "keyThrough": "targetId"
    },
    "readReceipts": {
      "type": "hasMany",
      "model": "read",
      "foreignKey": "sourceId",
      "through": "message_has_readreceipt",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
