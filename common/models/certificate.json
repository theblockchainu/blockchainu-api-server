{
  "name": "certificate",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "id": {
      "type": "string",
      "required": true
    },
    "stringifiedJSON": {
      "type": "string"
    },
    "stringifiedJSONWithoutSignature": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "peers": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_certificate",
      "keyThrough": "sourceId"
    }
  },
  "acls": [],
  "methods": {}
}
