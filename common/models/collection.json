{
  "name": "collection",
  "base": "PersistedModel",
  "idInjection": false,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "reviews",
        "comments",
        "bookmarks",
        "views"
      ]
    },
    "HasOne": {
      "relations": [
        {
          "model": "calendar",
          "hasManyRelation": "calendars",
          "autoCreate": true
        },
        {
          "model": "assessment_model",
          "hasManyRelation": "assessment_models",
          "autoCreate": true
        },
        {
          "model": "certificate_template",
          "hasManyRelation": "certificate_templates",
          "autoCreate": true
        }
      ]
    },
    "PatchMany": {
      "relations": [
        {
          "model": "topic",
          "hasManyRelation": "topics"
        }
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {
      "relationName": [
        "participants"
      ]
    }
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "type": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "stage": {
      "type": "string"
    },
    "language": {
      "type": [
        "string"
      ]
    },
    "headline": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "difficultyLevel": {
      "type": "string"
    },
    "prerequisites": {
      "type": "string"
    },
    "maxSpots": {
      "type": "string"
    },
    "videoUrls": {
      "type": [
        "string"
      ]
    },
    "imageUrls": {
      "type": [
        "string"
      ]
    },
    "totalHours": {
      "type": "number"
    },
    "price": {
      "type": "number"
    },
    "currency": {
      "type": "string"
    },
    "cancellationPolicy": {
      "type": "string"
    },
    "ageLimit": {
      "type": "string"
    },
    "aboutHost": {
      "type": "string"
    },
    "notes": {
      "type": "string"
    },
    "isApproved": {
      "type": "boolean",
      "default": false
    },
    "approvedBy": {
      "type": "string"
    },
    "isCanceled": {
      "type": "boolean",
      "default": false
    },
    "canceledBy": {
      "type": "string"
    },
    "status": {
      "type": "string",
      "default": "draft"
    },
    "submittedAt": {
      "type": "date"
    },
    "repeatPattern": {
      "type": "string"
    },
    "repeatFrequency": {
      "type": "string"
    },
    "repeatEndPattern": {
      "type": "string"
    },
    "repeatUntil": {
      "type": "string"
    },
    "academicGyan": {
      "type": "number",
      "default": 0
    },
    "nonAcademicGyan": {
      "type": "number",
      "default": 0
    },
    "subCategory": {
      "type": "string",
      "default": "workshop"
    },
    "resultsAnnounced": {
      "type": "boolean",
      "defaut": "false"
    }
  },
  "validations": [],
  "relations": {
    "views": {
      "type": "hasMany",
      "model": "view",
      "foreignKey": "sourceId",
      "through": "collection_has_view",
      "keyThrough": "targetId"
    },
    "topics": {
      "type": "hasMany",
      "model": "topic",
      "foreignKey": "sourceId",
      "through": "collection_has_topic",
      "keyThrough": "targetId"
    },
    "contents": {
      "type": "hasMany",
      "model": "content",
      "foreignKey": "sourceId",
      "through": "collection_has_content",
      "keyThrough": "targetId"
    },
    "reviews": {
      "type": "hasMany",
      "model": "review",
      "foreignKey": "sourceId",
      "through": "collection_has_review",
      "keyThrough": "targetId"
    },
    "participants": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "collection_has_participant",
      "keyThrough": "sourceId"
    },
    "owners": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_collection",
      "keyThrough": "sourceId"
    },
    "invites": {
      "type": "hasMany",
      "model": "peer_invite",
      "foreignKey": "sourceId",
      "through": "collection_has_invite",
      "keyThrough": "targetId"
    },
    "calendars": {
      "type": "hasMany",
      "model": "calendar",
      "foreignKey": "sourceId",
      "through": "collection_has_calendar",
      "keyThrough": "targetId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "sourceId",
      "through": "collection_has_comment",
      "keyThrough": "targetId"
    },
    "bookmarks": {
      "type": "hasMany",
      "model": "bookmark",
      "foreignKey": "sourceId",
      "through": "collection_has_bookmark",
      "keyThrough": "targetId"
    },
    "payments": {
      "type": "hasMany",
      "model": "transaction",
      "foreignKey": "sourceId",
      "through": "collection_has_payment",
      "keyThrough": "targetId"
    },
    "payoutrules": {
      "type": "hasMany",
      "model": "payoutrule",
      "foreignKey": "sourceId",
      "through": "collection_has_payoutrule",
      "keyThrough": "targetId"
    },
    "rooms": {
      "type": "hasMany",
      "model": "room",
      "foreignKey": "sourceId",
      "through": "collection_has_room",
      "keyThrough": "targetId"
    },
    "communities": {
      "type": "hasMany",
      "model": "community",
      "foreignKey": "targetId",
      "through": "community_has_collection",
      "keyThrough": "sourceId"
    },
    "preferences": {
      "type": "hasMany",
      "model": "preference",
      "foreignKey": "sourceId",
      "through": "collection_has_preference",
      "keyThrough": "targetId"
    },
    "packages": {
      "type": "hasMany",
      "model": "package",
      "foreignKey": "sourceId",
      "through": "collection_has_package",
      "keyThrough": "targetId"
    },
    "availability": {
      "type": "hasMany",
      "model": "availability",
      "foreignKey": "sourceId",
      "through": "collection_has_availability",
      "keyThrough": "targetId"
    },
    "provisions": {
      "type": "hasMany",
      "model": "provision",
      "foreignKey": "sourceId",
      "through": "collection_has_provision",
      "keyThrough": "targetId"
    },
    "token_payments": {
      "type": "hasMany",
      "model": "token_transaction",
      "foreignKey": "sourceId",
      "through": "collection_has_token_payment",
      "keyThrough": "targetId"
    },
    "assessment_models": {
      "type": "hasMany",
      "model": "assessment_model",
      "foreignKey": "sourceId",
      "through": "collection_has_assessment_model",
      "keyThrough": "targetId"
    },
    "collection_scholarships": {
      "type": "hasMany",
      "model": "scholarship",
      "foreignKey": "targetId",
      "through": "scholarship_has_allowed_collection",
      "keyThrough": "sourceId"
    },
    "certificate_templates": {
      "type": "hasMany",
      "model": "certificate_template",
      "foreignKey": "sourceId",
      "through": "collection_has_certificate_template",
      "keyThrough": "targetId"
    },
    "promoCodes": {
      "type": "hasMany",
      "model": "promoCode",
      "foreignKey": "sourceId",
      "through": "collection_has_promoCode",
      "keyThrough": "targetId"
    },
    "rewards": {
      "type": "hasMany",
      "model": "reward",
      "foreignKey": "sourceId",
      "through": "collection_has_reward",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
