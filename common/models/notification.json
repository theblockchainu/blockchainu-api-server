{
  "name": "notification",
  "plural": "notifications",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "id": {
      "type": "string",
      "id": true
    },
    "type": {
      "type": "string"
    },
    "title": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "actionUrl": {
      "type": [
        "string"
      ]
    },
    "seen": {
      "type": "boolean",
      "default": false
    },
    "new": {
      "type": "boolean",
      "default": true
    },
    "hidden": {
      "type": "boolean",
      "default": false
    }
  },
  "validations": [],
  "relations": {
    "peer": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "targetId",
      "through": "peer_has_notification",
      "keyThrough": "sourceId"
    },
    "actor": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "notification_has_actor",
      "keyThrough": "targetId"
    },
    "collection": {
      "type": "hasMany",
      "model": "collection",
      "foreignKey": "sourceId",
      "through": "notification_has_collection",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
