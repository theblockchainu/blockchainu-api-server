{
  "name": "submission",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "AddPeerRelation": {
      "relationName": [
        "comments",
        "upvotes"
      ]
    },
    "PatchMany": {
      "relations": [
        {
          "model": "topic",
          "hasManyRelation": "topics"
        }
      ]
    },
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "name": {
      "type": "string"
    },
    "description": {
      "type": "string"
    },
    "picture_url": {
      "type": "string"
    },
    "isPrivate": {
      "type": "boolean",
      "default": true
    },
    "file_url": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "contents": {
      "type": "hasMany",
      "model": "content",
      "foreignKey": "targetId",
      "through": "content_has_submission",
      "keyThrough": "sourceId"
    },
    "peer": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "submission_has_peer",
      "keyThrough": "targetId"
    },
    "topics": {
      "type": "hasMany",
      "model": "topic",
      "foreignKey": "sourceId",
      "through": "submission_has_topic",
      "keyThrough": "targetId"
    },
    "comments": {
      "type": "hasMany",
      "model": "comment",
      "foreignKey": "sourceId",
      "through": "submission_has_comment",
      "keyThrough": "targetId"
    },
    "upvotes": {
      "type": "hasMany",
      "model": "upvote",
      "foreignKey": "sourceId",
      "through": "submission_has_upvote",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}
