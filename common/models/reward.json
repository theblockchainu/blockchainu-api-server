{
  "name": "reward",
  "base": "PersistedModel",
  "idInjection": true,
  "options": {
    "validateUpsert": true
  },
  "mixins": {
    "TimeStamp": {
      "createdAt": "createdAt",
      "updatedAt": "updatedAt",
      "required": false
    },
    "GlobalUtils": {}
  },
  "properties": {
    "position": {
      "type": "number"
    },
    "value": {
      "type": "number"
    },
    "title": {
      "type": "string"
    },
    "currency": {
      "type": "string"
    },
    "url": {
      "type": "string"
    }
  },
  "validations": [],
  "relations": {
    "collections": {
      "type": "hasMany",
      "model": "collection",
      "foreignKey": "targetId",
      "through": "collection_has_reward",
      "keyThrough": "sourceId"
    },
    "winners": {
      "type": "hasMany",
      "model": "peer",
      "foreignKey": "sourceId",
      "through": "reward_has_participant",
      "keyThrough": "targetId"
    }
  },
  "acls": [],
  "methods": {}
}